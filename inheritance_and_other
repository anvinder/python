class animal:
    def walk(self):
        print ('Animal walks ')
    def talk(self):
        print ('animal can talk in code language')
    def body(self):
        print ('animals have a hunky dory body !')

class dog (animal):
    def bark(self):
        print ('dogs bark but they are cute')
    def pet(self):
        print('most common pet is a dog')
    def walk(self):
        print ('dogs walk like other animals ')
    def talk(self):
        print ('animal can talk in code language')

def main ():
    a=animal()
    d=dog()
    for obj in (a,d):
        obj.walk()
        obj.talk()

main()
